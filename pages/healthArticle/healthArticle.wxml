<!--pages/healthArticle/healthArticle.wxml-->
<view wx:for="{{msgList}}" wx:for-item='item' wx:key='key'>
  <view>
    <view>
      <image src="http://39.105.56.223/health-0.0.1-SNAPSHOT/{{item.pic}}" mode="scaleToFill"></image>
    </view>
    <text>标题:{{item.title}}\n</text>
    <text>导语:{{item.article}}\n</text>
    <text>作者:{{item.author}}</text>
  </view>
</view>
<text class="last" style="position:fixed; bottom:0;width: 750rpx;" wx:if="{{isLast}}">我也是有底线的</text>