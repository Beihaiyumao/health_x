<!--pages/user/healthArticle.wxml-->
<view>
     <view>
        <view class="weui-search-bar">
            <view class="weui-search-bar__form">
            <!-- 搜索框 -->
                <view class="weui-search-bar__box">
                    <icon class="weui-icon-search_in-box" type="search" size="14"></icon>
                    <input type="text" bindinput='search_title'maxlength='15'class="weui-search-bar__input" placeholder="请输入文章名称"/>
                </view>
            </view>
            <!-- 搜索按钮,调用搜索查询方法 -->
            <view class="weui-search-bar__cancel-btn" bindtap='searchMyArticle'>搜索</view>
        </view>
    </view>
</view>
<block wx:for="{{msgList}}" wx:for-item='item' wx:key='key'>
  <view class='user_list mt60 clearfix'>
    <text>{{item.title}}</text>
    <text>{{item.article}}</text>
  </view>
</block>