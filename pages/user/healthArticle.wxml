<!--pages/user/healthArticle.wxml-->
<view class='page_row' bindtap="searchMyArticle">
  <view class="search">
    <view class="df search_arr">
      <icon class="searchcion" size='20' type='search'></icon>
      <input class="" bindinput='search_title'  placeholder="请输入关键字" />
    </view>
  </view>
  <view class='sousuo' bindtap=''>搜索</view>
</view>
<block wx:for="{{msgList}}" wx:for-item='item' wx:key='key'>
  <view class='user_list mt60 clearfix'>
    <text>{{item.title}}</text>
    <text>{{item.article}}</text>
  </view>
</block>